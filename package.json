		"@slangroom/oauth": "^1.44.1",
		"@slangroom/pocketbase": "^1.44.1",
		"@slangroom/qrcode": "^1.43.1",
		"@slangroom/redis": "^1.44.1",
		"@slangroom/shell": "^1.46.0",
		"@slangroom/timestamp": "^1.46.0",
