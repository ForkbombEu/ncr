		"oracledb": "^6.5.1",
		"pg": "^8.13.0",
		"pg-hstore": "^2.3.4",
		"prom-client": "^15.0.0",
		"snowflake-sdk": "^2.0.2",
		"sqlite3": "^5.1.7",
		"tedious": "^18.2.0",
